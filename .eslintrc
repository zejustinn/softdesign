{
    "extends": [
        "airbnb-base",
        "prettier",
        "plugin:security/recommended"
    ],
    "plugins": [
        "mocha",
        "security"
    ],
    "env": {
        "node": true,
        "es2021": true,
        "mocha": true
    },
    "rules": {
        "import/extensions": [
            "error",
            "ignorePackages"
        ]
    }
}
